#
# A mixture of water and urea
#

tolerance 2.0
filetype pdb
output mixture.pdb

structure water.pdb 
  number 1000 
  inside box 0. 0. 0. 50. 50. 50. 
end structure

structure urea.pdb
  number 400
  inside box 0. 0. 0. 50. 50. 50. 
end structure

structure ectoine.pdb
  number 200
  inside box 0. 0. 0. 50. 50. 50. 
end structure
